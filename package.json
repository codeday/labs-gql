    "cpy-cli": "^3.1.1",
    "eslint": "^7.26.0",
    "prisma": "3",
    "swagger-typescript-api": "^13.0.22",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^5.2.2"
  }
}
