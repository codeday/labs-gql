{
  "name": "@codeday/labs-gql",
  "version": "2.0.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "author": "Tyler Menezes <tylermenezes@codeday.org>",
  "license": "Artistic-2.0",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run prisma && npm -s run swagger && tsc --skipLibCheck && cpy '**/*' '!**/*.ts' dist/ --cwd=src/ --no-overwrite --parents",
    "prisma": "prisma format && prisma generate",
    "dev": "tsx watch src/index.ts",
    "debug": "tsx src/index.ts",
    "send-event-recommendations": "tsx scripts/sendEventRecommendations.ts",
    "swagger": "rm src/badgr/Api.ts; swagger-typescript-api -p badgr-api-v2.yaml -n Api2.ts -o ./src/badgr; echo 'type json = JSON;' | cat - src/badgr/Api2.ts > src/badgr/Api.ts; rm src/badgr/Api2.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@codeday/uploader-node": "^1.0.1",
    "@elastic/elasticsearch": "^9.2.0",
    "@linear/sdk": "^64.0.0",
    "@prisma/client": "^5.14.0",
    "@slack/web-api": "^7.1.0",
    "@ts-stack/markdown": "1.4",
    "ajv": "^8.12.0",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.7.0",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.0",
    "cron": "^3.1.4",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.4.6",
    "debug": "^4.3.4",
    "diacritics": "^1.3.0",
    "diff": "^5.1.0",
    "dotenv": "^16.4.5",
    "elastic-builder": "^2.16.0",
    "express": "^4.19.0",
    "express-basic-auth": "^1.2.1",
    "front-matter": "^4.0.2",
    "graphql": "^16.9.0",
    "graphql-type-json": "^0.3.2",
    "graphql-ws": "^5.14.2",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "knuth-shuffle-seeded": "^1.0.6",
    "luxon": "^3.7.2",
    "nodemailer": "^6.9.8",
    "openai": "^4.25.0",
    "randomstring": "^1.3.0",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.1.14",
    "supports-color": "^9.4.0",
    "tmp": "^0.2.1",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@codeday/eslint-typescript-config": "^2.1.5",
    "@types/body-parser": "^1.19.4",
    "@types/debug": "^4.1.8",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.12.12",
    "@types/nodemailer": "^6.4.15",
    "@types/randomstring": "^1.3.0",
    "@types/tmp": "^0.2.3",
    "@types/ws": "^8.18.1",
    "cpy-cli": "^3.1.1",
    "eslint": "^8.57.0",
    "prisma": "^5.14.0",
    "swagger-typescript-api": "^13.0.22",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3"
  }
}
